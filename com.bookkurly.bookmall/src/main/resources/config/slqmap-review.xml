<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    
<mapper namespace="PurchaseReviewDAO">
	<insert id="insert" parameterType="PurchaseReview">
		insert into "purchase_review"
values("purchase_review_seq".nextval, sysdate, sysdate, #{purchaseReviewContent}, #{bookSeq}, #{customSeq},#{orderSeq}, #{orderSerialNum}, #{customId})
	
	</insert>


</mapper>